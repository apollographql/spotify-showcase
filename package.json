{
  "name": "react-apollo-spotify-demo",
  "private": true,
  "scripts": {
    "generate": "graphql-codegen",
    "start": "concurrently \"npm:start:*\"",
    "start:client": "cd client && npm start",
    "start:server": "nodemon --watch './**/*.ts' --exec 'ts-node' server.ts",
    "setup:client": "cd client && npm install",
    "setup:server": "cd server && npm install",
    "postinstall": "concurrently \"npm:setup:*\""
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.16.4",
    "@graphql-codegen/fragment-matcher": "^3.3.3",
    "@graphql-codegen/typescript": "^2.8.7",
    "@graphql-codegen/typescript-operations": "^2.5.12",
    "@graphql-codegen/typescript-resolvers": "^2.7.12",
    "@mdx-js/rollup": "^2.3.0",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@types/cors": "^2.8.13",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.14.6",
    "@types/node-fetch": "^2.6.2",
    "@types/ws": "^8.5.4",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.13",
    "babel-plugin-prismjs": "^2.1.0",
    "concurrently": "^7.6.0",
    "dotenv": "^16.0.3",
    "graphql-import-node": "^0.0.5",
    "nodemon": "^2.0.21",
    "postcss": "^8.4.21",
    "prettier": "^2.8.1",
    "remark-frontmatter": "^4.0.1",
    "remark-mdx-frontmatter": "^2.1.1",
    "tailwindcss": "^3.2.7",
    "ts-node": "^10.9.1",
    "type-fest": "^3.6.1",
    "typescript": "^4.9.5"
  },
  "prettier": {
    "bracketSpacing": true,
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5"
  },
  "dependencies": {
    "@apollo/datasource-rest": "^5.0.2",
    "@apollo/server": "^4.4.1",
    "@apollo/subgraph": "^2.3.3",
    "@wry/equality": "^0.5.3",
    "camelize": "^1.0.1",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.12.0",
    "lodash": "^4.17.21",
    "node-fetch": "^3.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rxjs": "^7.8.0",
    "sass": "^1.58.3",
    "ts-invariant": "^0.10.3",
    "vite": "^4.1.4",
    "ws": "^8.12.1"
  }
}
