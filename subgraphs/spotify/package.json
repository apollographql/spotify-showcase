{
  "name": "spotify-subgraph",
  "private": true,
  "description": "A template for TypeScript projects with pre-configured tooling",
  "version": "0.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/apollographql/subgraph-template-typescript-apollo-server"
  },
  "homepage": "https://github.com/apollographql/subgraph-template-typescript-apollo-server#readme",
  "bugs": {
    "url": "https://github.com/apollographql/subgraph-template-typescript-apollo-server/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=18.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --watch \"src/**\" --ext \"ts,json,graphql\" --exec 'ts-node' src/index.ts",
    "test": "jest",
    "lint:types": "tsc --noEmit"
  },
  "dependencies": {
    "@apollo/server": "^4.7.1",
    "@apollo/subgraph": "^2.4.7",
    "@graphql-tools/mock": "^9.0.0",
    "@graphql-tools/schema": "^10.0.0",
    "@sentry/node": "^7.59.3",
    "@wry/equality": "^0.5.6",
    "body-parser": "^1.20.2",
    "chalk": "^4.1.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "graphql-tag": "latest",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "querystring": "^0.2.1",
    "request": "^2.88.2",
    "rxjs": "^7.8.1",
    "spotify-api": "*",
    "ts-invariant": "^0.10.3",
    "type-fest": "^3.11.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@sentry/types": "^7.59.3",
    "@types/cors": "^2.8.13",
    "@types/jest": "^29.0.3",
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.17.1",
    "jest": "^29.0.3",
    "nodemon": "^2.0.15",
    "ts-jest": "^29.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "src"
    ],
    "globals": {
      "ts-jest": {
        "verbose": true
      }
    }
  }
}
